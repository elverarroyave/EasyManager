    <changeSet author="elverarroyave" id="monthly_payment_options">
        <preConditions onFail="MARK_RAN">
            <sqlCheck expectedResult="0">
                SELECT COUNT(1)
                FROM master_head
                WHERE name = 'MONTHLY_PAYMENT_OPTIONS'
            </sqlCheck>
        </preConditions>
        <insert tableName="master_head">
            <column name="id" valueNumeric="3"/>
            <column name="name" value="MONTHLY_PAYMENT_OPTIONS"/>
            <column name="description" value="Opciones de metodos de pagos mensuales"/>
            <column name="created_date" value="2024-07-12T12:12:51"/>
            <column name="update_date" value="2024-07-12T12:12:51"/>
        </insert>
    </changeSet>

